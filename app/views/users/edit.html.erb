<h1>Edit User</h1>
<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<br>
<%= f.label :height %>(inches)
<%= f.text_field :height %>
<br>
<br>
<%= f.label :happiness %>
<%= f.text_field :happiness %>
<br>
<br>
<%= f.label :nausea %>
<%= f.text_field :nausea %>
<br>
<br>
<%= f.label :tickets %>
<%= f.text_field :tickets %>
<br>
<br>
<%= f.submit %>
<% end %>
